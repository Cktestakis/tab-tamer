{
    "manifest_version": 3,
    "name": "Tab Tamer",
    "version": "1.0",
    "description": "Monitor and manage your open tabs effortlessly!",
    "permissions": ["tabs", "storage", "notifications"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "128": "icon.png"
    }
  }
  